<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="fr5_v6_robot">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import fr5_v6_robot urdf file -->
    <xacro:include filename="$(find fr5_v6_robot)/urdf/fr5_v6_robot.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="fr5_v6_robot.ros2_control.xacro" />


    <xacro:fr5_v6_robot_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
